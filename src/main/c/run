#!/bin/sh
cd `dirname $0`

if [ -x /usr/local/gcc/bin/gcc ]; then GCC=/usr/local/gcc/bin/gcc; else GCC=gcc; fi

${GCC} -O3 gen.c -o gen && ./gen

# -Wall


